import multiprocessing as mp
import time

def fun():
    while 1:
        time.sleep(3)
        print "1"
    pass

def fun2():
    while 1:
        time.sleep(2)
        print "2"
    pass



if __name__=="__main__":
    a=mp.Process(target=fun)
    b=mp.Process(target=fun2)
    a.start()
    b.start()